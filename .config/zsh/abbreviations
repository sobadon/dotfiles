abbr dein="vim ~/.config/nvim/dein.toml"
abbr deinlazy="vim ~/.config/nvim/dein_lazy.toml"
abbr ga="git add"
abbr gap="git add -p"
abbr gc="git commit"
abbr gd="git diff"
abbr gds="git diff --staged"
abbr gs="git status"
abbr gl="git log --graph --oneline"
abbr initvim="vim ~/.config/nvim/init.vim"
abbr k="kubectl"
abbr zshreload="source ~/.zshrc"
abbr jou="sudo journalctl --no-hostname"
abbr joul="sudo journalctl --no-hostname --since '1 minutes ago'"
abbr explorer.exe="/mnt/c/Windows/explorer.exe"
